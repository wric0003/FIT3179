{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Top 5 Countries by Arrivals",
    "data": {
        "url": "https://wric0003.github.io/FIT3179/js/top5_arrivals.csv"
    },
    "mark": {
        "type": "arc",
        "tooltip": true
    },
    "encoding": {
        "column": {
            "field": "Year"
        },
        "theta": {
            "field": "Arrivals",
            "type": "quantitative",
            "stack": "normalize"
        },
        "color": {
            "field": "Country",
            "type": "nominal"
        }
    }
}